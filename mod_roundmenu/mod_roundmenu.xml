	<authorEmail>admin@bixie.nl</authorEmail>
	<authorUrl>www.bixie.nl</authorUrl>
	<version>1.0.0</version>
	<description>Rond menu knopje</description>
	<files>
		<filename module="mod_roundmenu">mod_roundmenu.php</filename>
		<folder>assets</folder>
		<folder>tmpl</folder>
