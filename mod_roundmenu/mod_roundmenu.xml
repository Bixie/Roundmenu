				<field name="button3" type="menuitem" default="101" label="Menu geel" description=""/>
				<field name="none" type="menuitem" multiple="true" default="" size="5" label="Menu's geen kleur" description=""/>
			</fieldset>
		</fields>
	</config>
</extension>
